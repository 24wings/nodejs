{
    "tags": {
        "allowUnknownTags": false
    },
    "source": {
        "include": "public/js",
        "includePattern": ".js$",
        "excludePattern": "(node_modules/|docs)"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "opts": {
        "template": "public/assets/template/",
        "encoding": "utf8",
        "destination": "public/jsapi",
        "recurse": true,
        "verbose": true
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    }
}